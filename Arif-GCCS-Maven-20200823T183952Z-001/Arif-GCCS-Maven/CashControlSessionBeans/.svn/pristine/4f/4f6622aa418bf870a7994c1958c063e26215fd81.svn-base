<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE weblogic-ejb-jar PUBLIC "-//BEA Systems, Inc.//DTD WebLogic 7.0.0 EJB//EN" "http://www.bea.com/servers/wls700/dtd/weblogic-ejb-jar.dtd">
<weblogic-ejb-jar>
    <weblogic-enterprise-bean>
        <ejb-name>AdminDaoEJB</ejb-name>
        <transaction-descriptor>
            <trans-timeout-seconds>300</trans-timeout-seconds>
        </transaction-descriptor>
        <enable-call-by-reference>True</enable-call-by-reference>
        <jndi-name>AdminDaoEJB</jndi-name>
        <local-jndi-name>AdminDaoEJBLocal</local-jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>AdminFacadeEJB</ejb-name>
        <transaction-descriptor>
            <trans-timeout-seconds>300</trans-timeout-seconds>
        </transaction-descriptor>
        <enable-call-by-reference>True</enable-call-by-reference>
        <jndi-name>AdminFacadeEJB</jndi-name>
        <local-jndi-name>AdminFacadeLocal</local-jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>CommonOpsDaoEJB</ejb-name>
        <transaction-descriptor>
            <trans-timeout-seconds>300</trans-timeout-seconds>
        </transaction-descriptor>
        <enable-call-by-reference>True</enable-call-by-reference>
        <jndi-name>CommonOpsDaoEJB</jndi-name>
        <local-jndi-name>CommonOpsDaoEJBLocal</local-jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>CommonOpsFacadeEJB</ejb-name>
        <transaction-descriptor>
            <trans-timeout-seconds>300</trans-timeout-seconds>
        </transaction-descriptor>
        <enable-call-by-reference>True</enable-call-by-reference>
        <jndi-name>CommonOpsFacadeEJB</jndi-name>
        <local-jndi-name>CommonOpsFacadeEJBLocal</local-jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>PrepPoaDaoEJB</ejb-name>
        <transaction-descriptor>
            <trans-timeout-seconds>900</trans-timeout-seconds>
        </transaction-descriptor>
        <enable-call-by-reference>True</enable-call-by-reference>
        <jndi-name>PrepPoaDaoEJB</jndi-name>
        <local-jndi-name>PrepPoaDaoEJBLocal</local-jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>PrepPoaFacadeEJB</ejb-name>
        <transaction-descriptor>
            <trans-timeout-seconds>1000</trans-timeout-seconds>
        </transaction-descriptor>
        <reference-descriptor>
            <ejb-local-reference-description>
                <ejb-ref-name>ejb/OacLocalHome</ejb-ref-name>
                <jndi-name>OacEJB.OacLocalHome</jndi-name>
            </ejb-local-reference-description>
        </reference-descriptor>
        <enable-call-by-reference>True</enable-call-by-reference>
        <jndi-name>PrepPoaFacadeEJB</jndi-name>
        <local-jndi-name>PrepPoaFacadeEJBLocal</local-jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>RODDaoEJB</ejb-name>
        <transaction-descriptor>
            <trans-timeout-seconds>300</trans-timeout-seconds>
        </transaction-descriptor>
        <enable-call-by-reference>True</enable-call-by-reference>
        <jndi-name>RODDaoEJB</jndi-name>
        <local-jndi-name>RODDaoEJBLocal</local-jndi-name>
    </weblogic-enterprise-bean>
   <weblogic-enterprise-bean>
        <ejb-name>CODDaoEJB</ejb-name>
        <transaction-descriptor>
            <trans-timeout-seconds>300</trans-timeout-seconds>
        </transaction-descriptor>
        <enable-call-by-reference>True</enable-call-by-reference>
        <jndi-name>CODDaoEJB</jndi-name>
        <local-jndi-name>CODDaoEJBLocal</local-jndi-name>
    </weblogic-enterprise-bean>
   <weblogic-enterprise-bean>
        <ejb-name>FTCDaoEJB</ejb-name>
        <transaction-descriptor>
            <trans-timeout-seconds>300</trans-timeout-seconds>
        </transaction-descriptor>
        <enable-call-by-reference>True</enable-call-by-reference>
        <jndi-name>FTCDaoEJB</jndi-name>
        <local-jndi-name>FTCDaoEJBLocal</local-jndi-name>
    </weblogic-enterprise-bean>    
    <weblogic-enterprise-bean>
        <ejb-name>RODFacadeEJB</ejb-name>
        <transaction-descriptor>
            <trans-timeout-seconds>600</trans-timeout-seconds>
        </transaction-descriptor>
        <enable-call-by-reference>True</enable-call-by-reference>
        <jndi-name>RODFacadeEJB</jndi-name>
        <local-jndi-name>RODFacadeEJBLocal</local-jndi-name>
    </weblogic-enterprise-bean>
   <weblogic-enterprise-bean>
        <ejb-name>CODFacadeEJB</ejb-name>
        <transaction-descriptor>
            <trans-timeout-seconds>600</trans-timeout-seconds>
        </transaction-descriptor>
        <enable-call-by-reference>True</enable-call-by-reference>
        <jndi-name>CODFacadeEJB</jndi-name>
        <local-jndi-name>CODFacadeEJBLocal</local-jndi-name>
    </weblogic-enterprise-bean>    
   <weblogic-enterprise-bean>
        <ejb-name>FTCFacadeEJB</ejb-name>
        <transaction-descriptor>
            <trans-timeout-seconds>600</trans-timeout-seconds>
        </transaction-descriptor>
        <enable-call-by-reference>True</enable-call-by-reference>
        <jndi-name>FTCFacadeEJB</jndi-name>
        <local-jndi-name>FTCFacadeEJBLocal</local-jndi-name>
    </weblogic-enterprise-bean>        
    <weblogic-enterprise-bean>
        <ejb-name>SystemUtilsDaoEJB</ejb-name>
        <transaction-descriptor>
            <trans-timeout-seconds>600</trans-timeout-seconds>
        </transaction-descriptor>
        <enable-call-by-reference>True</enable-call-by-reference>
        <jndi-name>SystemUtilsDaoEJB</jndi-name>
        <local-jndi-name>SystemUtilsDaoEJBLocal</local-jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>SystemUtilsEJB</ejb-name>
        <transaction-descriptor>
            <trans-timeout-seconds>600</trans-timeout-seconds>
        </transaction-descriptor>
        <enable-call-by-reference>True</enable-call-by-reference>
        <jndi-name>SystemUtilsEJB</jndi-name>
        <local-jndi-name>SystemUtilsEJBLocal</local-jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>RIHMessageListener</ejb-name>
        <message-driven-descriptor>
            <pool>
                <max-beans-in-free-pool>5</max-beans-in-free-pool>
                <initial-beans-in-free-pool>1</initial-beans-in-free-pool>
            </pool>
            <destination-jndi-name>FXShip.FX.MDE.RLAC.Reply</destination-jndi-name>
        </message-driven-descriptor>
    </weblogic-enterprise-bean>
</weblogic-ejb-jar>
