package com.fedex.lacitd.cashcontrol.datatier.valueobject;

import java.util.*;

public class CountryCurrencyVO implements java.io.Serializable {

    private Integer _cntryCurrencyId;
    private String _currencyCd;
    private String _currencyNm;
    private Date _currencyValidSince;
    private String _currencySymbol;
    private double _exchRateMin;
    private double _exchRateMax;
    private String _countryCd;
    public CountryCurrencyVO() {
    }
    public CountryCurrencyVO(Integer cntryCurrencyId, String currencyCd, String currencyNm, Date currencyValidSince, String currencySymbol, double exchRateMin, double exchRateMax, String countryCd) {
        _cntryCurrencyId = cntryCurrencyId;
        _currencyCd = currencyCd;
        _currencyNm = currencyNm;
        _currencyValidSince = currencyValidSince;
        _currencySymbol = currencySymbol;
        _exchRateMin = exchRateMin;
        _exchRateMax = exchRateMax;
        _countryCd = countryCd;
    }
    /**
     * Set the value of cntryCurrencyId
     */
    public void setCntryCurrencyId(Integer cntryCurrencyId) {
        _cntryCurrencyId = cntryCurrencyId;
    }

    /**
     * Get the value of cntryCurrencyId
     */
    public Integer getCntryCurrencyId() {
        return _cntryCurrencyId;
    }

    /**
     * Set the value of currencyCd
     */
    public void setCurrencyCd(String currencyCd) {
        _currencyCd = currencyCd;
    }

    /**
     * Get the value of currencyCd
     */
    public String getCurrencyCd() {
        return _currencyCd;
    }

    /**
     * Set the value of currencyNm
     */
    public void setCurrencyNm(String currencyNm) {
        _currencyNm = currencyNm;
    }

    /**
     * Get the value of currencyNm
     */
    public String getCurrencyNm() {
        return _currencyNm;
    }

    /**
     * Set the value of currencyValidSince
     */
    public void setCurrencyValidSince(Date currencyValidSince) {
        _currencyValidSince = currencyValidSince;
    }

    /**
     * Get the value of currencyValidSince
     */
    public Date getCurrencyValidSince() {
        return _currencyValidSince;
    }

    /**
     * Set the value of currencySymbol
     */
    public void setCurrencySymbol(String currencySymbol) {
        _currencySymbol = currencySymbol;
    }

    /**
     * Get the value of currencySymbol
     */
    public String getCurrencySymbol() {
        return _currencySymbol;
    }

    /**
     * Set the value of exchRateMin
     */
    public void setExchRateMin(double exchRateMin) {
        _exchRateMin = exchRateMin;
    }

    /**
     * Get the value of exchRateMin
     */
    public double getExchRateMin() {
        return _exchRateMin;
    }

    /**
     * Set the value of exchRateMax
     */
    public void setExchRateMax(double exchRateMax) {
        _exchRateMax = exchRateMax;
    }

    /**
     * Get the value of exchRateMax
     */
    public double getExchRateMax() {
        return _exchRateMax;
    }

    /**
     * Set the value of countryCd
     */
    public void setCountryCd(String countryCd) {
        _countryCd = countryCd;
    }

    /**
     * Get the value of countryCd
     */
    public String getCountryCd() {
        return _countryCd;
    }

    /**
     * Get the value of the primary key
     */
    public Integer getCountryCurrencyPK() {
        return _cntryCurrencyId;
    }

    /**
     * Returns a string representation of the value object.
     */
    public String toString() {
        StringBuffer stringBuffer = new StringBuffer();
        stringBuffer.append("CntryCurrencyId: " + _cntryCurrencyId);
        stringBuffer.append("CurrencyCd: " + _currencyCd);
        stringBuffer.append("CurrencyNm: " + _currencyNm);
        stringBuffer.append("CurrencyValidSince: " + _currencyValidSince);
        stringBuffer.append("CurrencySymbol: " + _currencySymbol);
        stringBuffer.append("ExchRateMin: " + _exchRateMin);
        stringBuffer.append("ExchRateMax: " + _exchRateMax);
        stringBuffer.append("CountryCd: " + _countryCd);
        return stringBuffer.toString();
    }

}
