package com.fedex.lacitd.cashcontrol.datatier.valueobject;

import java.util.*;

public class ChkinAgtCommentVO implements java.io.Serializable {

    private Integer _commentId;
    private String _commentCd;
    private String _commentDesc;
    private String _applyTo;
    private String _status;
    public ChkinAgtCommentVO() {
    }
    public ChkinAgtCommentVO(Integer commentId, String commentCd, String commentDesc, String applyTo, String status) {
        _commentId = commentId;
        _commentCd = commentCd;
        _commentDesc = commentDesc;
        _applyTo = applyTo;
        _status = status;
    }
    /**
     * Set the value of commentId
     */
    public void setCommentId(Integer commentId) {
        _commentId = commentId;
    }

    /**
     * Get the value of commentId
     */
    public Integer getCommentId() {
        return _commentId;
    }

    /**
     * Set the value of commentCd
     */
    public void setCommentCd(String commentCd) {
        _commentCd = commentCd;
    }

    /**
     * Get the value of commentCd
     */
    public String getCommentCd() {
        return _commentCd;
    }

    /**
     * Set the value of commentDesc
     */
    public void setCommentDesc(String commentDesc) {
        _commentDesc = commentDesc;
    }

    /**
     * Get the value of commentDesc
     */
    public String getCommentDesc() {
        return _commentDesc;
    }

    /**
     * Set the value of applyTo
     */
    public void setApplyTo(String applyTo) {
        _applyTo = applyTo;
    }

    /**
     * Get the value of applyTo
     */
    public String getApplyTo() {
        return _applyTo;
    }

    /**
     * Set the value of status
     */
    public void setStatus(String status) {
        _status = status;
    }

    /**
     * Get the value of status
     */
    public String getStatus() {
        return _status;
    }

    /**
     * Get the value of the primary key
     */
    public Integer getChkinAgtCommentPK() {
        return _commentId;
    }

    /**
     * Returns a string representation of the value object.
     */
    public String toString() {
        StringBuffer stringBuffer = new StringBuffer();
        stringBuffer.append("CommentId: " + _commentId);
        stringBuffer.append("CommentCd: " + _commentCd);
        stringBuffer.append("CommentDesc: " + _commentDesc);
        stringBuffer.append("ApplyTo: " + _applyTo);
        stringBuffer.append("Status: " + _status);
        return stringBuffer.toString();
    }

}
